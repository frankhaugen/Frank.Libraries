<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <PackageTags>Extensions, AutoFixture, Test, OmitRecursion, Recursion</PackageTags>
        <Description>This contains a very useful extension method for AutoFixture that allows you to OmitRecursion, (if a class has multiple nested objects, and/or self-reference, this simple extension method allows you to provent this from being an issue)</Description>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="AutoFixture" Version="4.18.0" />
    </ItemGroup>

    <Import Project="..\..\Directory.Build.props" Condition="Exists('..\..\Directory.Build.props')"/>
</Project>

<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <LangVersion>10.0</LangVersion>
        <Nullable>enable</Nullable>
        <IsPackable>false</IsPackable>
        <RepositoryType>Git</RepositoryType>
        <RepositoryUrl>https://github.com/frankhaugen/Frank.Libraries</RepositoryUrl>
        <PackageProjectUrl>https://github.com/frankhaugen/Frank.Libraries</PackageProjectUrl>
        <Copyright>Copyright (c) 2020 Frank R. Haugen</Copyright>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
        <PackageIcon>icon.png</PackageIcon>
        <Authors>Frank R. Haugen</Authors>
        <PackageTags></PackageTags>
        <Description>Not very useful at the moment, but is intended to become a helpful tool for accessing device information</Description>
        <NoWarn>1701</NoWarn>
    </PropertyGroup>

    <ItemGroup>
        <None Include="..\icon.png">
            <Pack>True</Pack>
            <PackagePath></PackagePath>
        </None>
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="DeviceId" Version="6.0.0"/>
        <PackageReference Include="DeviceIOControlLib" Version="0.1.6"/>
        <PackageReference Include="EasyNetwork" Version="1.0.6"/>
        <PackageReference Include="H.Hooks" Version="1.4.14"/>
        <PackageReference Include="hidlibrary" Version="3.3.40"/>
        <PackageReference Include="HidSharp" Version="2.1.0"/>
        <PackageReference Include="ILGPU" Version="0.10.1"/>
        <PackageReference Include="IPNetwork2" Version="2.5.360"/>
        <PackageReference Include="LibreHardwareMonitorLib" Version="0.8.8"/>
        <PackageReference Include="Network" Version="6.2.5.23"/>
        <PackageReference Include="NodaTime" Version="3.0.9"/>
        <PackageReference Include="Xam.Plugin.Connectivity" Version="3.2.0"/>
        <PackageReference Include="Xam.Plugin.DeviceInfo" Version="4.1.1"/>
    </ItemGroup>
</Project>